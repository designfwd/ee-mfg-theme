{
  "name": "flying-trunk-theme",
  "version": "1.0.0",
  "description": "A WordPress theme for Flying Trunk",
  "homepage": "https://github.com/benjatron/flying-trunk-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjatron/flying-trunk-theme.git"
  },
  "author": "FWD Creative, LLC",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/benjatron/flying-trunk-theme/issues"
  },
  "paths": {
    "assets": {
      "base": "./assets/",
      "favicons": "./assets/favicons/",
      "fonts": "./assets/fonts/",
      "images": {
        "base": "./assets/images/",
        "dist": "./assets/images/dist/",
        "src": "./assets/images/src/"
      },
      "scripts": {
        "base": "./assets/scripts/",
        "dist": "./assets/scripts/dist/",
        "src": "./assets/scripts/src/"
      },
      "styles": {
        "base": "./assets/styles/",
        "dist": "./assets/styles/dist/",
        "src": "./assets/styles/src/"
      }
    },
    "scss": []
  },
  "vars": {
    "siteCSSName": "site.combined.min.css",
    "scssName": "main.scss",
    "cssName": "main.css"
  },
  "globs": {
    "distCSS": [
      "./node_modules/slick-carousel/slick/slick-theme.css"
    ],
    "distJS": [
      "./node_modules/slick-carousel/slick/slick.js",
      "./node_modules/lazysizes/lazysizes.js"
    ],
    "babelJS": [
      "./assets/scripts/src/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "fancy-log": "^1.3.0",
    "git-rev-sync": "^1.9.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-filter": "^5.0.1",
    "gulp-header": "^1.8.9",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.4.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "jquery": "^3.3.1",
    "moment": "^2.19.1",
    "node-sass": "^4.5.3"
  },
  "dependencies": {
    "bourbon": "^4.3.4",
    "jquery.mmenu": "^6.1.8",
    "lazysizes": "^4.0.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build"
  }
}
